{% block block_audio_two_col %}
    {% set element = block.slots.getSlot('audio1') %}

    <div class="col-12 col-md-6" data-cms-element-id="{{ element.id }}">
        {% block block_audio1_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>

    {% set element = block.slots.getSlot('audio2') %}

    <div class="col-12 col-md-6" data-cms-element-id="{{ element.id }}">
        {% block block_audio2_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>
{% endblock %}
