{% block block_headline_jost %}
    {% set element = block.slots.getSlot('textLeft') %}

    <div class="col-12 col-md-6" data-cms-element-id="{{ element.id }}">
        {% block block_textLeft_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>

    {% set element = block.slots.getSlot('textRight') %}

    <div class="col-12 col-md-6" data-cms-element-id="{{ element.id }}">
        {% block block_textRight_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>
{% endblock %}
